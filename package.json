{
	"name": "spotify-canvas-ts",
	"type": "module",
	"description": "A TypeScript library to fetch canvas links from Spotify.",
	"version": "0.0.2",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lkekana/spotify-canvas-ts.git"
	},
